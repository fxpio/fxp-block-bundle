{
    "name": "sonatra/block-bundle",
    "type": "symfony-bundle",
    "description": "Provides tools for defining UI blocks, rendering and mapping response data to related models",
    "keywords": ["block", "user interface", "ui"],
    "homepage": "https://github.com/sonatra/sonatra-block-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "sonatra/block": "^1.0.0"
    },
    "require-dev": {
        "symfony/twig-bundle": "^3.4.0|^4.0.0",
        "symfony/validator": "^3.4.0|^4.0.0",
        "symfony/templating": "^3.4.0|^4.0.0",
        "symfony/asset": "^3.4.0|^4.0.0",
        "doctrine/orm": "^2.6.0"
    },
    "suggest": {
        "sonatra/bootstrap-bundle": "The twitter bootstrap DSL twig tag"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\BlockBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
