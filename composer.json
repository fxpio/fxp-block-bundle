{
    "name": "fxp/block-bundle",
    "type": "symfony-bundle",
    "description": "Provides tools for defining UI blocks, rendering and mapping response data to related models",
    "keywords": ["block", "user interface", "ui"],
    "homepage": "https://github.com/fxpio/fxp-block-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^4.2.0",
        "fxp/block": "^1.1.0"
    },
    "require-dev": {
        "symfony/twig-bundle": "^4.2.0",
        "symfony/validator": "^4.2.0",
        "symfony/templating": "^4.2.0",
        "symfony/translation": "^4.2.0",
        "symfony/asset": "^4.2.0",
        "doctrine/orm": "^2.6.0"
    },
    "suggest": {
        "fxp/bootstrap-bundle": "The twitter bootstrap DSL twig tag"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\BlockBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
